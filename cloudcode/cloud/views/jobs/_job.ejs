<tr>  
  <td>
  <%- "<input type=\"checkbox\" name=\"" + job.id + "\" value=\"" + job.id + "\">"
  %>
  </td>
  <td><%= job.get('jobTitle') %></td>
  <td><%= job.createdAt.getMonth() + 1 + "/" + job.createdAt.getDate() + "/" + job.createdAt.getFullYear() %></td>  
  <td><%= job.get('company') %></td>  
  <td><%= job.get('city') %></td>  
  <td><%= job.get('educationRequirement') %></td>  
  <td><%= job.get('salary') %></td>  
  <td><%= job.get('numOpenings') %></td>  
  <td><%= job.get('hoursPerWeek') %></td>  
  <td><%= job.get('fullTime') %></td>
  <td><%= job.get('contact') %></td>
  <td>
  	<a href="/job/<%= job.id %>" >More</a>
  </td>
  <td>
  	<a href="/job/<%= job.id %>/edit" >Edit</a>
  </td>
  <td>
    <form id="delete<%= job.id %>" method="POST" action="/job/<%= job.id %>" onsubmit="return confirm('Are you sure you want to delete this job opening?')">
      <input type="hidden" name="_method" id="requestType" value="DELETE" />
      <a onclick="document.getElementById('delete<%= job.id %>').submit();">Delete</a>
    </form>
  </td>
</tr>